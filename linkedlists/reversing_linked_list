//Traverse through the list, put them in stack. print the stack

class Node(){
  int key;
  Node next;
}

class LinkedList(){
  
  Node head;
  LinkedList(){
    head = nw LinkedList();
  }
  
  public void reverse(){
    Node node = head;
    ArrayList<Node> a = new ArrayList<Node>();
    while(node!=null){
      a.add(node);
      node=node.next;
    }
    
    for(int i = node.size()-1; i>=0; i--){
      System.out.println (" "+node.get(i));      
    }
  }
}

--------Method 2--------

public void reverse(Node curr, Node prev){
  if (curr.next==null){
    head = curr;
    curr.next = prev;
    }
    
    Node next1 = curr.next
    curr.next = prev;
    reverse(next1, curr);
    return head;
}
